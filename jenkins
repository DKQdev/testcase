pipeline {
  agent any
    stages {
      stage('---Clean---') {
        steps {
          echo 'cleaning the code'
         }
        }
      stage('---test---') {
        steps {
          echo 'test the code'
          }
         }
        stage ('---package---') {
          steps {
            echo 'packing the code'
            }
           }
          }
         }
        
