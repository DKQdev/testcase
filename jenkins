pipeline {
  agent any
    stages {
      stage('---Clean---') {
        steps {
          echo 'Code cleaning'
         }
        }
      stage('---test---') {
        steps {
          echo 'Testing the Code'
          }
         }
        stage ('---package---') {
          steps {
            echo 'Turning the code to a package'
            }
           }
          }
         }
        
